USE Douglas_Williams

Create schema LabAssignment2

Create table LabAssignment2.Doctor
(
DoctorID Int Identity(10000,1) Primary Key not null ,
LicenceNo int unique not null,
Statel varchar(50) unique not null,
DoctorName varchar(50) not null,
DoctorYearOfGraduation date not null Check(DoctorYearOfGraduation > '1985')
)


